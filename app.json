{
  "expo": {
    "name": "teste",
    "slug": "teste",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/img_G2W.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/img_G2W.png",
      "resizeMode": "contain",
      "backgroundColor": "#242424"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/img_G2W.png",
        "backgroundColor": "#24242"
      },
      "package": "com.leohdhshshs.teste"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2fa5c4ff-a267-4550-bd74-1cfc3ea8e156"
      }
    }
  }
}
